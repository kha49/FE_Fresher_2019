.kem-nav
    .kem-nav__title overviews
    ul.kem-nav__list

        - var itemArr = [
        - {navLink: "Destinations", iconClass: "", navLinkSmall: ""},
        - {navLink: "experiences", iconClass: "fas fa-angle-down", navLinkSmall: [{navLv2: "wedding", navLv3: ""}, {navLv2: "gastronomy", navLv3: ""}, {navLv2: "spa", navLv3: ""}, {navLv2: "golf", navLv3: ""}, {navLv2: "ski", navLv3: ""}, {navLv2: "signature suites", navLv3: ""}, {navLv2: "my kempinski experience magazine", navLv3: ""}, {navLv2: "the world of kempinski brand book", navLv3: ""}]},
        - {navLink: "special offers", iconClass: "fas fa-angle-down", navLinkSmall: [{navLv2: "escape in style this summer", navLv3: ""}, {navLv2: "early booker", navLv3: ""}, {navLv2: "suitely sensational", navLv3: ""}, {navLv2: "wellness & spa offers", navLv3: ""}, {navLv2: "romance offers", navLv3: ""}, {navLv2: "family offers", navLv3: ""}, {navLv2: "golf offers", navLv3: ""}]},
        - {navLink: "kempinski loyalty", iconClass: "", navLinkSmall: ""},
        - {navLink: "meetings and events", iconClass: "fas fa-angle-down", navLinkSmall: [{navLv2: "inspiring offers", navLv3: ""}, {navLv2: "meeting & events", navLv3: ""}, {navLv2: "conventions & congresses", navLv3: ""}, {navLv2: "incentives", navLv3: ""}, {navLv2: "weddings", navLv3: ""}, {navLv2: "private events", navLv3: ""}, {navLv2: "mice factsheet", navLv3: ""}]},
        - {navLink: "blog", iconClass: "", navLinkSmall: ""},
        - {navLink: "about us", iconClass: "fas fa-angle-down", navLinkSmall: [{navLv2: "history", navLv3: ""}, {navLv2: "management board", navLv3: ""}, {navLv2: "corporate social responsibility", navLv3: ["be health", "company policies"]}, {navLv2: "lady in red", navLv3: ""}, {navLv2: "hotel development", navLv3: ""}, {navLv2: "kempinski residences", navLv3: ""}, {navLv2: "our partners", navLv3: ["airline partners", "automobile partners", "partner offers"]}, {navLv2: "culinary concepts", navLv3: ""}, {navLv2: "my kempinski experience magazine", navLv3: ""}, {navLv2: "the world of kempinski brand book", navLv3: ""}]},
        - {navLink: "#kempinski gallery", iconClass: "", navLinkSmall: ""},
        - {navLink: "english", iconClass: "fas fa-angle-down", navLinkSmall: [{navLv2: "ai cập", navLv3: ""}, {navLv2: "espanol", navLv3: ""}, {navLv2: "france", navLv3: ""}, {navLv2: "pyccknn", navLv3: ""}, {navLv2: "tàu", navLv3: ""}, {navLv2: "deutsch", navLv3: ""}]},
        - {navLink: "login to your account", iconClass: "", navLinkSmall: ""},
        - {navLink: "create a new account", iconClass: "", navLinkSmall: ""}
        - ]

        each item in itemArr
            li.kem-nav__item
                a.kem-nav__link= item.navLink
                span.kem-nav__icon-down-open
                    i(class= item.iconClass)
                ul.kem-nav__list-small
                    each smallItem in item.navLinkSmall
                        li.kem-nav__item-small
                            a.kem-nav__link-small= smallItem.navLv2
                            ul.kem-nav__list-small-lv3
                                each smallItemLv3 in smallItem.navLv3
                                    li.kem-nav__item-small-lv3
                                        a.kem-nav__link-small-lv3= smallItemLv3