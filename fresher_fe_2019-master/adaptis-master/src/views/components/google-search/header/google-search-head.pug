.google-search-header
    .google-search-header__banner
        img.google-search-header(src="https://www.google.com/images/branding/googlelogo/2x/googlelogo_color_92x30dp.png", alt="banner")
    .google-search-header__content
        .google-search-header__search-box
            input.input(type="text")
            i.fas.fa-keyboard
            img(src="data:image/png;base64,", alt="alt")
            a Tìm kiếm
        .google-search-header__slidebar-box
            - var slide = ['Tất cả','Hình ảnh', 'Video', 'Tin tức', 'Thêm'] 
            - var tool = ['Cài đặt','Công cụ']
            - var icon = ['fas fa-search', 'far fa-image', 'fab fa-youtube','far fa-newspaper', 'fas fa-ellipsis-v']
            +slidebar('slidebar', 'google-search-header__slidebar', slide, icon)
            +slidebar('slidebar', 'google-search-header__slidebar', tool)
    .google-search-header__avatar-box
        i.fas.fa-th
        img(src="http://thuthuatphanmem.vn/uploads/2018/09/11/hinh-anh-dep-10_044127763.jpg", alt="")