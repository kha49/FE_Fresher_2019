extends ./layouts/master.pug

block title
  title My Page - Index

block content
  - var pages = { homePage: '/homepage', testPage: '/test', Google: '/google', searchResultGoogle: '/searchResultGoogle', airbnb: '/airbnb', airbnbUpdate: '/airbnbUpdate', adaptisVer2: './adaptisVer2', adaptisVer2Contact: './adaptisVer2Contact'};

  //- Desktop page
  section.page.home__page
    h1.page__title # Index page3

    ul(style="list-style: disc; padding-left: 4rem; margin: 4rem 0")
      each val, index in pages
        li
          a(href=val)= index